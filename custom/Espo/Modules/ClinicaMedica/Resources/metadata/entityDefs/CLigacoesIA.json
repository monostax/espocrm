{
    "fields": {
        "name": {
            "type": "varchar",
            "required": false,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "paciente": {
            "type": "link"
        },
        "status": {
            "type": "enum",
            "required": true,
            "options": [
                "Agendada",
                "Ligando",
                "Em Progresso",
                "Recusada",
                "Finalizada",
                "Cancelada"
            ],
            "style": {
                "Agendada": null,
                "Ligando": "warning",
                "Em Progresso": "warning",
                "Recusada": "danger",
                "Finalizada": "success",
                "Cancelada": "info"
            },
            "default": "Agendada",
            "isSorted": true,
            "displayAsLabel": true,
            "labelType": "state",
            "maxLength": 100,
            "isCustom": true
        },
        "consultaMedica": {
            "type": "link"
        },
        "tipo": {
            "type": "enum",
            "options": [
                "Feedback de Consulta"
            ],
            "style": {
                "Feedback de Consulta": null
            },
            "default": "Feedback de Consulta",
            "maxLength": 100,
            "isCustom": true
        },
        "incomingWebhooks": {
            "type": "linkMultiple"
        },
        "transcript": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "preview": true,
            "readOnly": true,
            "inlineEditDisabled": true,
            "isCustom": true
        },
        "callCost": {
            "type": "currency",
            "decimal": true,
            "readOnly": true,
            "inlineEditDisabled": true,
            "isCustom": true
        },
        "recordingUrl": {
            "type": "url",
            "copyToClipboard": true,
            "readOnly": true,
            "isCustom": true
        },
        "callCostCurrency": {
            "default": "USD"
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "meetings": {
            "type": "hasMany",
            "entity": "Meeting",
            "foreign": "parent"
        },
        "calls": {
            "type": "hasMany",
            "entity": "Call",
            "foreign": "parent"
        },
        "tasks": {
            "type": "hasChildren",
            "entity": "Task",
            "foreign": "parent"
        },
        "emails": {
            "type": "hasChildren",
            "entity": "Email",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "paciente": {
            "type": "belongsTo",
            "foreign": "ligacoesIAs",
            "entity": "CPaciente",
            "audited": false,
            "isCustom": true
        },
        "consultaMedica": {
            "type": "belongsTo",
            "foreign": "ligacoesIAs",
            "entity": "CConsultaMedica",
            "audited": false,
            "isCustom": true
        },
        "incomingWebhooks": {
            "type": "hasMany",
            "foreign": "cLigacoesIA",
            "entity": "IncomingWebhook",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}