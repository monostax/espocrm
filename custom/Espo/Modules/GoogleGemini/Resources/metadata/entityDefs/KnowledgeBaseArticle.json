{
    "fields": {
        "geminiIndexStatus": {
            "type": "enum",
            "options": ["NotIndexed", "Pending", "Indexed", "Failed"],
            "default": "NotIndexed",
            "readOnly": true
        },
        "geminiDocumentName": {
            "type": "varchar",
            "maxLength": 512,
            "readOnly": true,
            "utility": true,
            "tooltip": true,
            "tooltipText": "Full document resource name in Gemini File Search for article body (e.g., fileSearchStores/team-xxx/documents/doc-yyy)"
        },
        "geminiAttachmentDocuments": {
            "type": "jsonArray",
            "readOnly": true,
            "utility": true,
            "tooltip": true,
            "tooltipText": "Array of Gemini document names for indexed attachments. Each entry contains: documentName, attachmentId, attachmentName"
        },
        "geminiIndexedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "geminiLastProcessedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "geminiIndexError": {
            "type": "text",
            "readOnly": true,
            "utility": true
        },
        "geminiFileSearchStore": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/link"
        }
    },
    "links": {
        "geminiFileSearchStore": {
            "type": "belongsTo",
            "entity": "GeminiFileSearchStore",
            "foreign": "knowledgeBaseArticles"
        },
        "geminiUploadOperations": {
            "type": "hasMany",
            "entity": "GeminiFileSearchStoreUploadOperation",
            "foreign": "knowledgeBaseArticle"
        }
    }
}

