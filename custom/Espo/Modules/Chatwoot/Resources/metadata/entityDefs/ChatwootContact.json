{
    "deleteId": true,
    "fields": {
        "deleteId": {
            "type": "varchar",
            "maxLength": 17,
            "default": "0",
            "readOnly": true
        },
        "name": {
            "type": "varchar",
            "maxLength": 255,
            "readOnly": true
        },
        "contactName": {
            "type": "foreign",
            "relation": "contact",
            "foreign": "name",
            "readOnly": true
        },
        "email": {
            "type": "email",
            "tooltip": true
        },
        "identifier": {
            "type": "varchar",
            "maxLength": 255,
            "tooltip": true
        },
        "chatwootInboxId": {
            "type": "int",
            "tooltip": true
        },
        "chatwootCreatedAt": {
            "type": "datetime",
            "readOnly": true,
            "tooltip": true
        },
        "chatwootLastActivityAt": {
            "type": "datetime",
            "readOnly": true,
            "tooltip": true
        },
        "availabilityStatus": {
            "type": "enum",
            "options": ["online", "offline"],
            "readOnly": true
        },
        "blocked": {
            "type": "bool",
            "default": false,
            "readOnly": true
        },
        "avatarUrl": {
            "type": "text",
            "readOnly": true,
            "tooltip": true,
            "view": "chatwoot:views/chatwoot-contact/fields/avatar-url"
        },
        "contact": {
            "type": "link",
            "required": false,
            "tooltip": true
        },
        "phoneNumber": {
            "type": "varchar",
            "required": false,
            "tooltip": true,
            "maxLength": 50
        },
        "chatwootContactId": {
            "type": "int",
            "tooltip": true,
            "readOnly": true
        },
        "chatwootAccount": {
            "type": "link",
            "required": true,
            "tooltip": true
        },
        "lastSyncedAt": {
            "type": "datetime",
            "readOnly": true,
            "tooltip": true
        },
        "syncStatus": {
            "type": "enum",
            "options": ["pending", "synced", "error", "merged", "deleted"],
            "default": "pending",
            "tooltip": true,
            "view": "views/fields/enum-styled",
            "style": {
                "pending": "default",
                "synced": "success",
                "error": "danger",
                "merged": "warning",
                "deleted": "default"
            }
        },
        "mergedIntoChatwootContactId": {
            "type": "int",
            "readOnly": true,
            "tooltip": true
        },
        "syncError": {
            "type": "text",
            "readOnly": true,
            "tooltip": true
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams",
            "required": true,
            "readOnly": true
        }
    },
    "links": {
        "contact": {
            "type": "belongsTo",
            "entity": "Contact",
            "foreign": "chatwootContacts"
        },
        "chatwootAccount": {
            "type": "belongsTo",
            "entity": "ChatwootAccount",
            "foreign": "contacts"
        },
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "contactInboxes": {
            "type": "hasMany",
            "entity": "ChatwootContactInbox",
            "foreign": "chatwootContact"
        },
        "conversations": {
            "type": "hasMany",
            "entity": "ChatwootConversation",
            "foreign": "chatwootContact"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "chatwootContactIdAccount": {
            "unique": true,
            "columns": ["chatwootContactId", "chatwootAccountId", "deleteId"]
        },
        "phoneNumberAccount": {
            "columns": ["phoneNumber", "chatwootAccountId"]
        },
        "contactId": {
            "columns": ["contactId"]
        },
        "createdAt": {
            "columns": ["createdAt"]
        }
    },
    "cascadeDelete": {
        "links": ["contactInboxes", "conversations"]
    }
}

