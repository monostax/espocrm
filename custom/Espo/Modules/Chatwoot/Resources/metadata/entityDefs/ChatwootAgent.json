{
    "deleteId": true,
    "fields": {
        "deleteId": {
            "type": "varchar",
            "maxLength": 17,
            "default": "0",
            "readOnly": true
        },
        "name": {
            "type": "varchar",
            "maxLength": 255,
            "required": true
        },
        "email": {
            "type": "email",
            "required": true
        },
        "password": {
            "type": "password",
            "dbType": "text",
            "required": true,
            "tooltip": true,
            "notStorable": true
        },
        "availableName": {
            "type": "varchar",
            "maxLength": 255
        },
        "role": {
            "type": "enum",
            "options": ["agent", "administrator"],
            "default": "agent",
            "required": true
        },
        "availabilityStatus": {
            "type": "enum",
            "options": ["available", "busy", "offline"],
            "default": "offline"
        },
        "autoOffline": {
            "type": "bool",
            "default": true
        },
        "isAI": {
            "type": "bool",
            "default": false,
            "tooltip": true
        },
        "contextualTransferIsEnabled": {
            "type": "bool",
            "default": false
        },
        "transferScenarios": {
            "type": "text",
            "rowsMin": 10
        },
        "aiPrompt": {
            "type": "wysiwyg",
            "tooltip": true,
            "audited": true,
            "auditView": "chatwoot:views/chatwoot-agent/fields/ai-prompt-audit-diff"
        },
        "knowledgeBaseCategories": {
            "type": "linkMultiple",
            "tooltip": true
        },
        "calendarsToManage": {
            "type": "linkMultiple",
            "tooltip": true
        },
        "confirmed": {
            "type": "bool",
            "default": false,
            "readOnly": true
        },
        "avatarUrl": {
            "type": "url"
        },
        "customRoleId": {
            "type": "int",
            "readOnly": true
        },
        "chatwootAgentId": {
            "type": "int",
            "readOnly": true,
            "tooltip": true
        },
        "chatwootAccount": {
            "type": "link",
            "required": true
        },
        "chatwootUser": {
            "type": "link"
        },
        "chatwootTeams": {
            "type": "linkMultiple"
        },
        "chatwootInboxes": {
            "type": "linkMultiple"
        },
        "syncStatus": {
            "type": "enum",
            "options": ["pending", "synced", "error"],
            "default": "pending"
        },
        "lastSyncedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "lastSyncError": {
            "type": "text",
            "readOnly": true
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams",
            "required": true,
            "readOnly": true
        },
        "assignedUser": {
            "type": "link",
            "view": "views/fields/assigned-user",
            "tooltip": true
        }
    },
    "links": {
        "chatwootAccount": {
            "type": "belongsTo",
            "entity": "ChatwootAccount",
            "foreign": "agents"
        },
        "chatwootUser": {
            "type": "belongsTo",
            "entity": "ChatwootUser",
            "foreign": "agents"
        },
        "chatwootTeams": {
            "type": "hasMany",
            "entity": "ChatwootTeam",
            "relationName": "chatwootAgentChatwootTeam",
            "foreign": "chatwootAgents"
        },
        "knowledgeBaseCategories": {
            "type": "hasMany",
            "entity": "KnowledgeBaseCategory",
            "relationName": "chatwootAgentKnowledgeBaseCategory",
            "foreign": "chatwootAgents"
        },
        "calendarsToManage": {
            "type": "hasMany",
            "entity": "User",
            "relationName": "chatwootAgentCalendarUser",
            "foreign": "chatwootAgentsWithCalendarAccess"
        },
        "chatwootInboxes": {
            "type": "hasMany",
            "entity": "ChatwootInbox",
            "relationName": "chatwootAgentChatwootInbox",
            "foreign": "chatwootAgents"
        },
        "wahaSessionLabels": {
            "type": "hasMany",
            "entity": "WahaSessionLabel",
            "foreign": "agent"
        },
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "uniqueAgent": {
            "unique": true,
            "columns": ["chatwootAgentId", "chatwootAccountId", "deleteId"]
        },
        "chatwootAccountId": {
            "columns": ["chatwootAccountId"]
        },
        "email": {
            "columns": ["deleted"]
        },
        "createdAt": {
            "columns": ["createdAt"]
        },
        "createdAtId": {
            "unique": true,
            "columns": ["createdAt", "id"]
        }
    }
}
