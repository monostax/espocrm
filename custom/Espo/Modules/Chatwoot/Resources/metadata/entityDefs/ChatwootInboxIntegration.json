{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "maxLength": 255
        },
        "phoneNumber": {
            "type": "varchar",
            "maxLength": 50,
            "tooltip": true
        },
        "status": {
            "type": "enum",
            "options": [
                "DRAFT",
                "CREATING",
                "PENDING_QR",
                "CONNECTING",
                "ACTIVE",
                "DISCONNECTED",
                "FAILED"
            ],
            "default": "DRAFT",
            "readOnly": true
        },
        "channelType": {
            "type": "enum",
            "options": ["whatsappQrcode", "whatsappCloudApi"],
            "default": "whatsappQrcode",
            "readOnlyAfterCreate": true
        },
        "wahaPlatform": {
            "type": "link",
            "readOnly": true,
            "utility": true,
            "layoutDetailDisabled": true,
            "layoutListDisabled": true,
            "layoutMassUpdateDisabled": true
        },
        "chatwootAccount": {
            "type": "link",
            "required": true,
            "readOnlyAfterCreate": true
        },
        "wahaSessionName": {
            "type": "varchar",
            "maxLength": 255,
            "readOnly": true,
            "tooltip": true
        },
        "wahaSessionId": {
            "type": "varchar",
            "maxLength": 255,
            "readOnly": true,
            "notStorable": true
        },
        "wahaAppId": {
            "type": "varchar",
            "maxLength": 255,
            "readOnly": true
        },
        "chatwootInboxId": {
            "type": "int",
            "readOnly": true
        },
        "chatwootInboxIdentifier": {
            "type": "varchar",
            "maxLength": 255,
            "readOnly": true
        },
        "whatsappId": {
            "type": "varchar",
            "maxLength": 50,
            "readOnly": true,
            "tooltip": true
        },
        "whatsappName": {
            "type": "varchar",
            "maxLength": 255,
            "readOnly": true
        },
        "errorMessage": {
            "type": "text",
            "readOnly": true
        },
        "connectedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "lastActivityAt": {
            "type": "datetime",
            "readOnly": true
        },
        "qrCode": {
            "type": "text",
            "notStorable": true,
            "readOnly": true,
            "layoutDetailDisabled": false,
            "layoutListDisabled": true,
            "layoutEditDisabled": true,
            "view": "chatwoot:views/chatwoot-inbox-integration/fields/qr-code"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "team": {
            "type": "link",
            "readOnly": true
        }
    },
    "links": {
        "wahaPlatform": {
            "type": "belongsTo",
            "entity": "WahaPlatform",
            "foreign": "chatwootInboxIntegrations"
        },
        "chatwootAccount": {
            "type": "belongsTo",
            "entity": "ChatwootAccount",
            "foreign": "chatwootInboxIntegrations"
        },
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "team": {
            "type": "belongsTo",
            "entity": "Team"
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": ["name", "deleted"]
        },
        "status": {
            "columns": ["status", "deleted"]
        },
        "createdAt": {
            "columns": ["createdAt"]
        },
        "createdAtId": {
            "unique": true,
            "columns": ["createdAt", "id"]
        }
    }
}
