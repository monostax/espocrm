{
    "deleteId": true,
    "fields": {
        "deleteId": {
            "type": "varchar",
            "maxLength": 17,
            "default": "0",
            "readOnly": true
        },
        "name": {
            "type": "varchar",
            "required": true,
            "maxLength": 255
        },
        "description": {
            "type": "text"
        },
        "allowAutoAssign": {
            "type": "bool",
            "default": true,
            "tooltip": true
        },
        "account": {
            "type": "link",
            "required": true
        },
        "chatwootTeamId": {
            "type": "int",
            "readOnly": true,
            "tooltip": true
        },
        "isMember": {
            "type": "bool",
            "readOnly": true,
            "tooltip": true
        },
        "syncStatus": {
            "type": "enum",
            "options": ["pending", "synced", "error"],
            "default": "pending"
        },
        "lastSyncedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "lastSyncError": {
            "type": "text",
            "readOnly": true
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams",
            "required": true
        },
        "chatwootAgents": {
            "type": "linkMultiple"
        }
    },
    "links": {
        "account": {
            "type": "belongsTo",
            "entity": "ChatwootAccount",
            "foreign": "chatwootTeams"
        },
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "chatwootAgents": {
            "type": "hasMany",
            "entity": "ChatwootAgent",
            "relationName": "chatwootAgentChatwootTeam",
            "foreign": "chatwootTeams"
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": ["name", "deleteId"]
        },
        "account": {
            "columns": ["accountId", "deleteId"]
        },
        "uniqueTeam": {
            "unique": true,
            "columns": ["chatwootTeamId", "accountId", "deleteId"]
        },
        "createdAt": {
            "columns": ["createdAt"]
        },
        "createdAtId": {
            "unique": true,
            "columns": ["createdAt", "id"]
        }
    }
}
