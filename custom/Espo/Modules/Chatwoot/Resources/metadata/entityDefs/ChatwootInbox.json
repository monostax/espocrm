{
    "deleteId": true,
    "fields": {
        "deleteId": {
            "type": "varchar",
            "maxLength": 17,
            "default": "0",
            "readOnly": true
        },
        "name": {
            "type": "varchar",
            "maxLength": 255,
            "required": true
        },
        "chatwootInboxId": {
            "type": "int",
            "required": true,
            "readOnly": true
        },
        "chatwootAccount": {
            "type": "link",
            "required": true
        },
        "channelType": {
            "type": "varchar",
            "maxLength": 100
        },
        "websiteUrl": {
            "type": "url"
        },
        "phoneNumber": {
            "type": "varchar",
            "maxLength": 50
        },
        "provider": {
            "type": "varchar",
            "maxLength": 100
        },
        "medium": {
            "type": "varchar",
            "maxLength": 100
        },
        "greetingEnabled": {
            "type": "bool",
            "default": false
        },
        "greetingMessage": {
            "type": "text"
        },
        "avatarUrl": {
            "type": "url"
        },
        "inboxIdentifier": {
            "type": "varchar",
            "maxLength": 255,
            "tooltip": true
        },
        "lastSyncedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams",
            "required": true,
            "readOnly": true
        },
        "chatwootAgents": {
            "type": "linkMultiple"
        },
        "chatwootInboxIntegration": {
            "type": "link"
        }
    },
    "links": {
        "chatwootAccount": {
            "type": "belongsTo",
            "entity": "ChatwootAccount",
            "foreign": "inboxes"
        },
        "contactInboxes": {
            "type": "hasMany",
            "entity": "ChatwootContactInbox",
            "foreign": "inbox"
        },
        "conversations": {
            "type": "hasMany",
            "entity": "ChatwootConversation",
            "foreign": "inbox"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "chatwootAgents": {
            "type": "hasMany",
            "entity": "ChatwootAgent",
            "relationName": "chatwootAgentChatwootInbox",
            "foreign": "chatwootInboxes"
        },
        "chatwootInboxIntegration": {
            "type": "belongsTo",
            "entity": "ChatwootInboxIntegration",
            "foreign": "chatwootInbox"
        }
    },
    "indexes": {
        "uniqueInbox": {
            "unique": true,
            "columns": ["chatwootInboxId", "chatwootAccountId", "deleteId"]
        },
        "chatwootAccountId": {
            "columns": ["chatwootAccountId"]
        }
    },
    "cascadeDelete": {
        "links": ["conversations", "contactInboxes"],
        "junctionTables": [
            {"table": "ChatwootAgentChatwootInbox", "column": "chatwootInboxId"}
        ]
    }
}






