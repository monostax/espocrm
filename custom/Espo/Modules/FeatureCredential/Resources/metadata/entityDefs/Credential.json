{
  "fields": {
    "name": {
      "type": "varchar",
      "required": true,
      "maxLength": 255,
      "trim": true
    },
    "credentialType": {
      "type": "link",
      "required": true,
      "view": "views/fields/link"
    },
    "oAuthAccount": {
      "type": "link",
      "view": "views/fields/link"
    },
    "credentialTypeCategory": {
      "type": "foreign",
      "link": "credentialType",
      "field": "category"
    },
    "config": {
      "type": "text",
      "view": "feature-credential:views/credential/fields/config"
    },
    "metadata": {
      "type": "jsonObject",
      "view": "feature-credential:views/credential/fields/metadata"
    },
    "expiresAt": {
      "type": "datetime"
    },
    "isActive": {
      "type": "bool",
      "default": true
    },
    "lastUsedAt": {
      "type": "datetime",
      "readOnly": true
    },
    "lastRotatedAt": {
      "type": "datetime"
    },
    "rotationReminderDays": {
      "type": "int",
      "default": 30,
      "min": 1
    },
    "lastHealthCheckAt": {
      "type": "datetime",
      "readOnly": true
    },
    "lastHealthCheckStatus": {
      "type": "enum",
      "options": [
        "",
        "healthy",
        "unhealthy",
        "unknown"
      ],
      "default": "",
      "readOnly": true,
      "style": {
        "healthy": "success",
        "unhealthy": "danger",
        "unknown": "warning"
      }
    },
    "description": {
      "type": "text"
    },
    "createdAt": {
      "type": "datetime",
      "readOnly": true
    },
    "modifiedAt": {
      "type": "datetime",
      "readOnly": true
    },
    "createdBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "modifiedBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "assignedUser": {
      "type": "link",
      "required": false,
      "view": "views/fields/assigned-user"
    },
    "teams": {
      "type": "linkMultiple",
      "view": "views/fields/teams",
      "required": true
    }
  },
  "links": {
    "credentialType": {
      "type": "belongsTo",
      "foreign": "credentials",
      "entity": "CredentialType",
      "required": true
    },
    "oAuthAccount": {
      "type": "belongsTo",
      "entity": "OAuthAccount",
      "foreign": "credentials"
    },
    "history": {
      "type": "hasMany",
      "foreign": "credential",
      "entity": "CredentialHistory"
    },
    "createdBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "assignedUser": {
      "type": "belongsTo",
      "entity": "User"
    },
    "teams": {
      "type": "hasMany",
      "entity": "Team",
      "relationName": "entityTeam",
      "layoutRelationshipsDisabled": true
    }
  },
  "collection": {
    "orderBy": "createdAt",
    "order": "desc"
  },
  "indexes": {
    "credentialTypeId": {
      "columns": [
        "credentialTypeId"
      ]
    },
    "expiresAt": {
      "columns": [
        "expiresAt"
      ]
    },
    "isActive": {
      "columns": [
        "isActive"
      ]
    },
    "oAuthAccountId": {
      "columns": [
        "oAuthAccountId"
      ]
    },
    "assignedUserId": {
      "columns": [
        "assignedUserId"
      ]
    }
  }
}