{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "maxLength": 255,
            "trim": true
        },
        "order": {
            "type": "int",
            "default": 10,
            "min": 1,
            "tooltip": true
        },
        "iconClass": {
            "type": "varchar",
            "maxLength": 100
        },
        "color": {
            "type": "varchar",
            "maxLength": 7
        },
"tabList": {
            "type": "jsonArray",
            "view": "global:views/settings/fields/tab-list"
        },
        "isDefault": {
            "type": "bool",
            "default": false,
            "tooltip": true
        },
        "isDisabled": {
            "type": "bool",
            "default": false,
            "tooltip": true
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        }
    },
    "links": {
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        }
    },
    "collection": {
        "orderBy": "order",
        "order": "asc"
    },
    "indexes": {
        "order": {
            "columns": ["order"]
        }
    }
}
